{
  "name": "react-agent",
  "version": "0.0.4",
  "description": "React Agent synchronizes client-side state and server-side data.  It can be included in any React project without conflict with other state management tools or REST APIs.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "test": "npm run client-test; npm run server-test",
    "client-test": "./node_modules/.bin/mocha --require babel-core/register ./tests/client-tests.js",
    "server-test": "./node_modules/.bin/mocha --require babel-core/register ./tests/server-tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yokyak/react-agent.git"
  },
  "author": "Andrew madic Harris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yokyak/react-agent/issues"
  },
  "homepage": "https://github.com/yokyak/react-agent#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "express": "^4.16.2",
    "jsdom": "^11.5.1",
    "mocha": "^4.1.0",
    "pg": "^7.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {},
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  }
}
